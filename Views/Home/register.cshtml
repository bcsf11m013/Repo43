							</div>
							<div class="panel-body text-center">
							<!-- Registration Form Starts -->
                                 <div class="col-md-6" style="margin-top:10px">
                        @using (Html.BeginForm("addMember", "Home", FormMethod.Post))
                        {
                            <div class="col-md-9" style="margin-left:200px">
                            <table>
                                
                                    
                            <tr><td>@Html.Label("UserName", new { @class = "col-md-4", style = "font-size:large;margin-top:20px" }) </td>
                                        
                                    <td>@Html.TextBoxFor(model => model.username, new { name="username",id="n"})
                                        @Html.ValidationMessageFor(m=>m.username)<input type="button" id="b1" value="Check" /><div id="result"></div></td></tr>
                                    
                                  <tr><td>@Html.Label("Email", new { @class = "col-md-4", style = "font-size:large;margin-top:20px" }) </td>
                                        
                                    <td>@Html.TextBoxFor(model => model.email, new { name="email"})
                                        @Html.ValidationMessageFor(m=>m.email)
                                    </td></tr>

                                <tr><td>@Html.Label("Phone", new { @class = "col-md-4", style = "font-size:large;margin-top:20px" }) </td>
                                        
                                    <td>@Html.TextBoxFor(model => model.phone, new { name="phone"})</td></tr>

                                <tr><td>@Html.Label("Country", new { @class = "col-md-4", style = "font-size:large;margin-top:20px" }) </td>
                                        
                                    <td>@Html.TextBoxFor(model => model.country, new { name="country"})</td></tr>

                             <tr> <td>@Html.Label("Pasword", new { @class = "col-md-4", style = "font-size:large;margin-top:20px" }) </td>
                                   <td>@Html.TextBoxFor(model => model.pswd, new { name="pswd"})
                                        @Html.ValidationMessageFor(m=>m.pswd) 
                                   </td></tr>
                             
                                       
                        

                                
                            </table>
                                <br />
                                <br />
                                <input type="submit" class="btn btn-primary" value="register" style="margin-left:100px"/>
                                </div>
                             

                        }
