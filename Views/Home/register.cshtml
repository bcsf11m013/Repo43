                                    </div>
                            </div>
           </div>
						@Scripts.Render("~/bundles/jquery")
    <script>

        $(document).ready(function () {

          //  $('#checking').hide();

            $('#b1').click(function () {
               
                var userName = $('#n').val();

                //alert('User Name ='+ userName);
                $('#checking').show();
                $.getJSON("/home/CheckUserName?n=" + userName, function (data)
                {
                    $('#checking').hide();
                    if (data) {
                        $('#result').text("User already exists in database");
                        
                    }
                    else {
                        $('#result').text("User Name is available");
                    }
                });

            });

        });
    </script>
